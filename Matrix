#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin>>n;
    int arr[101][101];
    for(int i=1;i<=n;i++){
        for(int j=1;j<=n;j++){
            cin>>arr[i][j];
        }
    }
    int primary=0,secondary=0;
    for(int i=1;i<=n;i++){
        primary+=arr[i][i];
        secondary+=arr[i][n-i+1];
    }
    cout<<abs(primary-secondary)<<endl;
    return 0;
}
