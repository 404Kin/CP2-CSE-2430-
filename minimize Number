#include<bits/stdc++.h>
using namespace std;
int main(){
        int n;
        cin>>n;
        vector<int>arr(201);
        for(int i=0;i<n;i++)cin>>arr[i];
        int op=0;
        while(true){
            bool even=true;
            for(int i=0;i<n;i++){
                if(arr[i]%2!=0){
                    even=false;
                    break;
                }
            }
            if(!even){
                break;
            }
            for(int i=0;i<n;i++){
                arr[i]/=2;
            }
            op++;
        }
        cout<<op<<endl;
    return 0;
}
