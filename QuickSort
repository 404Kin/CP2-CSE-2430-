
#include<bits/stdc++.h>
using namespace std;

void quickSort(int arr[],int low,int high){
    if(low<high){
        int pivot=arr[high];

        int i=low,j=low-1;

        while(i<=high){
            if(arr[i]<=pivot){
                j++;
                swap(arr[i],arr[j]);
            }
            i++;
        }
        int pi=j;

        quickSort(arr,low,pi-1);
        quickSort(arr,pi+1,high);

    }
}

int main() {
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }

    quickSort(arr, 0, n - 1);

    cout << "Sorted array: ";
    for (int i = 0; i < n; i++) cout << arr[i] << " ";
    return 0;
}
