#include<bits/stdc++.h>
using namespace std;
int main(){
    int n,m;
    cin>>n>>m;
    int A[n],freq[m]={0};
    for (int i=0;i<n;i++){
        cin>>A[i];
    }
    for (int i=0;i<n;i++){
        freq[A[i]-1]++;
    }
    for (int i=0;i<m;i++){
            cout<<freq[i]<<endl;
    }
    return 0;
}
