#include<bits/stdc++.h>
using namespace std;
int main(){
    int n,m;
    cin>>n>>m;
    vector<int>A(n),B(m);
    for(int i=0;i<n;i++)cin>>A[i];
    for(int i=0;i<m;i++)cin>>B[i];
    int i=0,j=0;
    while(i<n&&j<m){
        if(A[i]==B[j]){
            j++;
        }
        i++;
    }
    if(j==m)cout<<"YES"<<endl;
    else cout<<"NO"<<endl;
    return 0;
}
